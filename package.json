{
  "name": "@nazariazargul/rest-mocker",
  "version": "1.0.3",
  "description": "CLI tool to mock REST endpoints from OpenAPI specifications",
  "preferGlobal": true,
  "bin": {
    "rest-mock": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepack": "npm run build",
    "release:patch": "npm run build && npm version patch && git push && git push --tags",
    "release:minor": "npm run build && npm version minor && git push && git push --tags",
    "release:major": "npm run build && npm version major && git push && git push --tags"
  },
  "keywords": [
    "api",
    "mock",
    "openapi",
    "swagger",
    "cli",
    "rest",
    "testing",
    "development",
    "global"
  ],
  "author": {
    "name": "Hazar Gul Nazari"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AzarguNazari/rest-mock"
  },
  "bugs": {
    "url": "https://github.com/AzarguNazari/rest-mock/issues"
  },
  "homepage": "https://github.com/AzarguNazari/rest-mock#readme",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "commander": "^14.0.3",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "js-yaml": "^4.1.1",
    "pino": "^10.3.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.5",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "openapi-types": "^12.1.3",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
